{
  "name": "surgeup-design-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "package",
    "docs"
  ],
  "scripts": {
    "dev": "npm run build:lib && npm run dev -w docs",
    "build": "npm run build -w package && npm run build -w docs",
    "build:lib": "npm run build -w package",
    "build:docs": "npm run build -w docs",
    "dev:lib": "npm run dev -w package",
    "dev:docs": "npm run dev -w docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/vue3-vite": "^9.1.3",
    "sass": "^1.77.8",
    "storybook": "^9.1.3",
    "unplugin-vue-components": "^0.27.4",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
